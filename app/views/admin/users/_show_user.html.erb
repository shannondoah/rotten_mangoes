<div class="col-md-3">
<h2><%= link_to user.full_name, admin_user_path(user) %></h2>
  <p>Email: <%= user.email %></p>
  <p>Created at: <%= formatted_date(user.created_at) %> | Updated at: <%= formatted_date(user.updated_at) %></p>
  <%= link_to "delete", admin_user_path(user), :method => "delete"%> | <%= link_to "edit", edit_admin_user_path(user)%> | <%#= link_to 'switch', switch_admin_user_path(user) %>
      <%= link_to 'switch', [:switch, :admin, user], method: :put %>
</div>